{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "trailingSlash": false,
  "cleanUrls": true,
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "CACHE_BUSTER": "2025-01-07-hashrouter-final"
  }
}
